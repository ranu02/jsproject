		"version": "1.0.0",
		"title": "Swagger Petstore",
		"termsOfService": "http://swagger.io/terms/",
		"contact": {
			"email": "ayushkymar746@gmail.com"
		},
		"license": {
			"name": "Apache 2.0",
			"url": "http://www.apache.org/licenses/LICENSE-2.0.html"
		}
	},
	"host": "localhost:5003/",
	"basePath": "",
	"tags": [{
		"name": "onboard",
		"description": "Onboard user",
		"externalDocs": {
			"description": "Email and Password required field",
			"url": "localhost:5003/"
		}
	}],
	"schemes": [
		"http",
		"https"
	],
	"paths": {
		"/onboard": {
			"post": {
				"tags": [
					"onboard"
				],
				"summary": "User sign-up process",
				"description": "User sign-up process.",
				"operationId": "onboard",
				"consumes": "application/x-www-form-urlencoded",
				"parameters": [{
						"in": "formData",
						"name": "email",
						"description": "user email",
						"required": true,
						"schema": {
							"$ref": "#/definitions/Onboard"
						}
					},
					{
						"in": "formData",
						"name": "password",
						"description": "user password",
						"required": true,
						"schema": {
							"$ref": "#/definitions/Onboard"
						}
					}
				],
				"responses": {
					"200": {
						"description": "successful operation",
						"schema": {
							"$ref": "#/definitions/bookRequest"
						}
					}
				},
				"security": [{
					"petstore_auth": [
						"write:pets",
						"read:pets"
					]
				}]
			}
		}
	},
	"securityDefinitions": {
		"petstore_auth": {
			"type": "oauth2",
			"authorizationUrl": "http://petstore.swagger.io/oauth/dialog",
			"flow": "implicit",
			"scopes": {
				"write:pets": "modify pets in your account",
				"read:pets": "read your pets"
			}
		},
		"api_key": {
			"type": "apiKey",
			"name": "api_key",
			"in": "header"
		}
	},
	"definitions": {
		"Onboard": {
			"type": "object",
			"required": [
				"email",
				"password"
			],
			"properties": {
				"email": {
					"type": "string",
					"example": "test@yopmail.com"
				},
				"password": {
					"type": "string",
					"example": "12345678"
				}
			},
			"xml": {
				"name": "Onboard"
			}
		},
		"bookRequest": {
		"type": "object",
		"properties": {
		  "user_id": {
		    "type": "string",
		    "format": "string"
		  },
		  "timestamp": {
		    "type": "string",
		    "format": "number"
		  }
		}
	      },
		"ApiResponse": {
			"type": "object",
			"properties": {
				"code": {
					"type": "integer",
					"format": "int32"
				},
				"type": {
					"type": "string"
				},
				"message": {
					"type": "string"
				}
			}
		}
	},
	"externalDocs": {
		"description": "Find out more about Swagger",
		"url": "/"
	}
}
