{
  "swagger": "2.0",
  "info": {
    "description": "This is a sample server Petstore server.  You can find out more about     Swagger at [http://swagger.io](http://swagger.io) or on [irc.freenode.net, #swagger](http://swagger.io/irc/).      For this sample, you can use the api key `special-key` to test the authorization     filters.",
    "version": "1.0.0",
    "title": "Swagger Petstore",
    "termsOfService": "http://swagger.io/terms/",
    "contact": {
      "email": "ayushkymar746@gmail.com"
    },
    "license": {
      "name": "Apache 2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
    }
  },
  "host": "localhost:5003/",
  "basePath": "",
  "tags": [
    {
      "name": "onboard",
      "description": "Onboard user",
      "externalDocs": {
        "description": "Email and Password required field",
        "url": "localhost:5003/"
      }
    },
    {
      "name": "store",
      "description": "Access to Petstore orders"
    },
    {
      "name": "user",
      "description": "Operations about user",
      "externalDocs": {
        "description": "Find out more about our store",
        "url": "localhost:5003/"
      }
    }
  ],
  "schemes": [
    "http",
    "https"
  ],
  "paths": {
    "/onboard": {
      "post": {
        "tags": [
          "onboard"
        ],
        "summary": "User sign-up process",
        "description": "User sign-up process.",
        "operationId": "onboard",
        "consumes": "application/x-www-form-urlencoded",
        "parameters": [
          {
            "in": "formData",
            "name": "email",
            "description": "user email",
            "required": true,
            "schema": {
              "$ref": "#/definitions/Onboard"
            }
          },
          {
            "in": "formData",
            "name": "password",
            "description": "user password",
            "required": true,
            "schema": {
              "$ref": "#/definitions/Onboard"
            }
          }
        ],
        "responses": {
        "200": {
          "description": "successful operation",
          "schema": {
            "aerpass-dev-user"
          },
          "properties": {
            "user_id": {
                 "type": "string",
                  "description": "test_user_id"
                }
          },
        }
       },
        "security": [
          {
            "petstore_auth": [
              "write:pets",
              "read:pets"
            ]
          }
        ]
      },
    "User": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer",
          "format": "int64"
        },
        "username": {
          "type": "string"
        },
        "firstName": {
          "type": "string"
        },
        "lastName": {
          "type": "string"
        },
        "email": {
          "type": "string"
        },
        "password": {
          "type": "string"
        },
        "phone": {
          "type": "string"
        },
        "userStatus": {
          "type": "integer",
          "format": "int32",
          "description": "User Status"
        }
      },
      "xml": {
        "name": "User"
      }
    },
    "Tag": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer",
          "format": "int64"
        },
        "name": {
          "type": "string"
        }
      },
      "xml": {
        "name": "Tag"
      }
    },
    "Onboard": {
      "type": "object",
      "required": [
        "email",
        "password"
      ],
      "properties": {
        "email": {
          "type": "string",
          "example": "test@yopmail.com"
        },
        "password": {
          "type": "string",
          "example": "12345678"
        }
      },
      "xml": {
        "name": "Onboard"
      }
    },
    "ApiResponse": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "type": {
          "type": "string"
        },
        "message": {
          "type": "string"
        }
      }
    }
  },
  "externalDocs": {
    "description": "Find out more about Swagger",
    "url": "localhost:5003/"
  }
}
